{
  "name": "@mobilabs/kapp",
  "version": "1.9.1",
  "dbVersion": "1.9.1",
  "description": "A boilerplate to build a basic app server tuned to run into kubernetes",
  "main": "",
  "bin": {
    "kapp": "./bin/kapp.js"
  },
  "scripts": {
    "general": " --- GENERAL ---",
    "start": "node server/start.js",
    "app": "node server/start.js",

    "kustart": "cross-env KAPP_ENV_KUBE_YAML=true node server/start.js",

    "testing": " --- TESTING --- ",
    "test": "cross-env NODE_ENV=test c8 --reporter=lcov mocha ./test/main.js --exit",
    "display:coverage": "open -a safari ./coverage/lcov-report/index.html",
    "check:coverage": "c8 check-coverage --statements 70 --branches 70 --functions 50 --lines 70",
    "report": "c8 report",

    "deployment": " --- DEPLOYMENT --- ",
    "dep:prod": "sh tasks/prod.sh _prod-$npm_package_version && node tasks/prod.js _prod-$npm_package_version",
    "dep:private:package": "sh tasks/private.sh && sh tasks/compress.sh ${npm_package_name} ${npm_package_version} @mobilabs",

    "doc:site:web": " --- DOCUMENT WEB SITE --- ",
    "doc:site:init": "kasar xxx orion run once only",
    "doc:site:build": "kasar build",
    "doc:site:start": "kasar serve",

    "others": " --- OTHERS ---",
    "prepare": "husky install",
    "doc": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/kapp.git"
  },
  "keywords": [],
  "author": {
    "name": "Mobilabs",
    "email": "contact@mobilabs.fr",
    "url": "http://www.mobilabs.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/kapp/issues"
  },
  "c8": {
    "include": [
      "server/**/*.js"
    ],
    "exclude": [],
    "all": true
  },
  "homepage": "https://github.com/jclo/kapp",
  "dependencies": {
    "@mobilabs/kzlog": "^1.0.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb": "^6.3.0",
    "mysql2": "^3.7.0",
    "nopt": "^7.2.0",
    "picodb": "^1.0.5",
    "shelljs": "^0.8.5",
    "sqlite3": "^5.1.7",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "c8": "^9.0.0",
    "chai": "^4.4.0",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "mocha": "^10.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false
}
