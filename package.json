{
  "name": "@mobilabs/kapp",
  "version": "1.0.1",
  "description": "A boilerplate for a slim Node.js App server",
  "main": "index.js",
  "bin": {
    "kapp": "./bin/kapp.js"
  },
  "scripts": {
    "app": "node server/start.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha ./test/main.js",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "check-coverage": "nyc check-coverage --statements 30 --branches 0 --functions 0 --lines 30",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report",
    "doc": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/kapp.git"
  },
  "keywords": [],
  "author": {
    "name": "Mobilabs",
    "email": "contact@mobilabs.fr",
    "url": "http://www.mobilabs.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/kapp/issues"
  },
  "homepage": "https://github.com/jclo/kapp",
  "dependencies": {
    "@mobilabs/kasar": "^1.0.2",
    "@mobilabs/kzlog": "^1.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "nopt": "^5.0.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.3.0",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "open": "^7.2.1",
    "opener": "^1.5.2",
    "request": "^2.88.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run check-coverage && npm run report"
    }
  }
}
